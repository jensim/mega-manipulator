query ($cursor: String) {
  currentUser {
    assignedMergeRequests(after: $cursor, state:opened) {
      count
      pageInfo {
        hasNextPage
        startCursor
        endCursor
      }
      nodes {
        id
        iid
        webUrl
        title
        description
        sourceBranch
        targetBranch
        sourceProject {
          id
          path
          fullPath
          group {
            path
          }
          sshUrlToRepo
          httpUrlToRepo
          webUrl
        }
        targetProject {
          id
          path
          fullPath
          group {
            path
          }
          sshUrlToRepo
          httpUrlToRepo
          webUrl
        }
      }
    }
  }
}
